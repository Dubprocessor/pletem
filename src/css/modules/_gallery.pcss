.gallery{
    display: flex;
    border: solid;
    background: black;
    
    &__slot{
        width: 25%;
        display: grid; 
    }

    &__img{
        
       /*align-self: stretch; */
        
        @media all and (-ms-high-contrast: none), (-ms-high-contrast: active) {
            /*IE CSS styles go here */
            height: 100%;  
       }
       @supports (-ms-ime-align:auto) {
        /* Edge CSS styles go here */ 
        height: 100%;
      }
      
    }
}